{
  "name": "webgl-iridescence-twerk",
  "version": "0.0.0",
  "description": "Some cool ass iridescent effect",
  "repository": "https://github.com/marcofugaro/webgl-iridescence-twerk",
  "author": {
    "name": "Marco Fugaro",
    "email": "marco.fugaro@gmail.com",
    "url": "http://marcofugaro.it"
  },
  "license": "MIT",
  "scripts": {
    "postinstall": "yarn install-tree",
    "install-tree": "hash brew && ! hash tree && brew install tree || true",
    "start": "NODE_ENV=development webpack-dev-server",
    "prebuild": "yarn clean; yarn copy-public",
    "build": "NODE_ENV=production webpack",
    "clean": "rm -rf build",
    "copy-public": "cp -r public build",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.25%",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 Chrome version",
      "last 1 Firefox version",
      "last 1 Safari version"
    ]
  },
  "dependencies": {
    "ccapture.js": "Unsigno/ccapture.js",
    "controls-gui": "^2.0.0",
    "controls-state": "rreusser/controls-state",
    "datauritoblob": "^1.0.0",
    "detect-gpu": "^3.0.0",
    "glsl-blend": "^1.0.3",
    "glsl-hsl2rgb": "^1.1.0",
    "glsl-hypot": "^1.0.2",
    "glsl-noise": "^0.0.0",
    "highest-power-two": "^1.0.2",
    "image-promise": "^7.0.1",
    "lodash": "^4.17.20",
    "p-map": "^4.0.0",
    "postprocessing": "marcofugaro/postprocessing#dev",
    "pretty-ms": "^5.0.0",
    "simplex-noise": "^2.4.0",
    "stats.js": "^0.17.0",
    "three": "0.124.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "babel-loader": "^8.2.2",
    "babel-plugin-glsl": "^1.0.0",
    "babel-preset-accurapp": "^5.1.0",
    "chalk": "^4.1.0",
    "detect-port": "^1.3.0",
    "eslint-config-accurapp": "^5.1.0",
    "event-hooks-webpack-plugin": "^2.2.0",
    "glslify": "^7.1.1",
    "glslify-loader": "^2.0.0",
    "html-webpack-plugin": "4.5.1",
    "indent-string": "^4.0.0",
    "raw-loader": "^4.0.2",
    "react-dev-utils": "^11.0.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "3.11.1",
    "webpack-merge": "^4.2.2"
  }
}
